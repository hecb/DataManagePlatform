package com.huaxia.system.bean;

import java.io.Serializable;
import java.util.List;

import com.huaxia.base.bean.NativeID;

/**
 * @description 权限 bean
 * @author : zhangy
 * @date : 2016年12月22日 上午11:06:14
 */
public class Permission extends NativeID implements Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	
	private String name;
	
	private String action;
	
	private String module;
	
	private Long pid;
	
	private String seq;
	
	private String type;
	
	private List<Permission> permList;
	
	

	public Permission() {
		super();
		// TODO Auto-generated constructor stub
	}

	
	public Permission(String name, String action, String module, Long pid, String seq, String type) {
		super();
		this.name = name;
		this.action = action;
		this.module = module;
		this.pid = pid;
		this.seq = seq;
		this.type = type;
	}


	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getAction() {
		return action;
	}

	public void setAction(String action) {
		this.action = action;
	}

	public String getModule() {
		return module;
	}

	public void setModule(String module) {
		this.module = module;
	}

	public Long getPid() {
		return pid;
	}

	public void setPid(Long pid) {
		this.pid = pid;
	}

	public String getSeq() {
		return seq;
	}

	public void setSeq(String seq) {
		this.seq = seq;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}


	public List<Permission> getPermList() {
		return permList;
	}


	public void setPermList(List<Permission> permList) {
		this.permList = permList;
	}

	
	
}
