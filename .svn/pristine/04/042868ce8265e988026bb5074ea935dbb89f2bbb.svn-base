package com.huaxia.utils;

import org.apache.commons.lang.StringUtils;

/**
 * @description 类型转换工具
 * @author : zhangy
 * @date : 2016年12月29日 下午1:43:12
 */
public class ConvertTypeUtil {
	public static Integer StringToInt(String param){
		try {
			if(StringUtils.isNotEmpty(param)){
				return Integer.parseInt(param);
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
		return null;
	}
}
